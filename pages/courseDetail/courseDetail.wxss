

.m-name {
    font-size: 34rpx;
    color: #222;
    margin-bottom: 14rpx;
}

.m-tag {
    display: flex;
}

.m-tag>view {
    line-height: 44rpx;
    background: #ebebeb;
    color: #030303;
    padding: 0 24rpx;
    border-radius: 90px;
    font-size: 24rpx;
    margin-right: 10rpx;
}

.m-img {
    width: 150rpx;
    height: 150rpx;
    border-radius: 100px;
    overflow: hidden;
    margin-right: 20rpx;
}

.m-img image {
    width: 150rpx;
    height: 150rpx;
}

.m-item {
    background: #fff;
    display: flex;
    align-items: center;
    margin: 30rpx;
    border-radius: 16rpx;
}

.m-cont {
    flex: 1;
}

.m-bar {
    display: flex;
    justify-content: space-between;
    margin: 30rpx 0;
    color: #9b9b9b;
    font-size: 25rpx;
}

.m-like {
    font-size: 40rpx;
    color: #333;
    margin-left: 20rpx;
}

.m-intro {
    font-size: 26rpx;
    line-height: 1.6;
    color: #333;
    margin: 0 30rpx;
}

.m-head {
    padding: 10rpx 30rpx;
}

.d-cover {
    width: 750rpx;
    height: 388rpx;
}

.d-list >view {
    padding: 35rpx 30rpx;
    font-size: 28rpx;
    color: #333;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.d-list >view:last-child::after{
    border:none;
    background:transparent;
}
.d-list >view.active .ico{
    color:#af1654;
}
.d-list >view text {
    flex: 1;
}

.d-list .ico {
    display: flex;
    align-items: center;
    color: #666;
    font-size: 26rpx;
}

.d-list .ico::before {
    font-size: 40rpx;
    margin-right: 8rpx;
}
.d-title{
    font-size: 34rpx;
    color:#333;
}
.d-amt{
    font-size: 34rpx;
    color:#333;
    display: flex;
    align-items:center;
}
.d-amt text{
    font-size:24rpx;
    color:#9B9B9B;
    margin-right:20rpx;
}
.d-head{
    margin:30rpx;
    padding:30rpx 0;
}