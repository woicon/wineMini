.copuon-item {
    display: flex;
    height: 200rpx;
    flex-wrap: wrap;
    background: #fff;
    box-shadow: 2px 3px 8px 0 #ebebeb;
    margin: 30rpx;
    border-radius: 10rpx;
}


.c-button {
    width: 160rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 200rpx;
}

.c-num {
    font-size: 20rpx;
    margin-top: 10rpx;
}

.c-share {
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 22rpx;
    margin-top: 20rpx;
}

.c-share .ico {
    font-size: 44rpx;
}

.c-cont {
    flex: 1;
    padding: 0 30rpx;
}

.c-title {
    font-size: 32rpx;
    color: #333;
}

.c-info {
    color: #999;
    font-size: 22rpx;
}

.c-head {
    background: #af1654 url(data:image/png;base64,) repeat-y left;
    background-size: 12rpx;
    color: #fff;
    width: 200rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    font-size: 30rpx;
}
.coupon-item.disable .c-head{
    background-image: url(data:image/png;base64,);

}

.c-head text {
    font-size: 88rpx;
}

.c-price {
    display: flex;
    align-items: center;
}

.tab {
    display: flex;
    align-items: center;
    font-size: 34rpx;
    justify-content: center;
}

.tab>view {
    padding: 28rpx 30rpx;
    position: relative;
}

.tab>view.active {
    font-weight: 800;
    color: #af1654;
}

.tab>view.active::before {
    width: 58rpx;
    height: 6rpx;
    background: #af1654;
    border-radius: 90px;
    content: '';
    display: block;
    position: absolute;
    left: 50%;
    margin-left: -29rpx;
    bottom: 0;
}

.sku-item {
    margin: 30rpx;
}

.sku-img {
    width: 200rpx;
    height: 200rpx;
    margin-right:30rpx;
    border-radius: 8rpx;
}

.sku-img image {
    width: 200rpx;
    height: 200rpx;
    border-radius: 8rpx;
}

.sku-content {
    display: flex;
    flex-wrap: wrap;
    margin-top: 16rpx;
}

.sku-content {
    display: flex;
}

.sku-cont {
    flex: 1;
    display: flex;
    align-items:left;
    flex-direction:column;
    justify-content:space-between;
}

.sku-head {
    background: #af1654;
    color: #fff;
    height: 44rpx;
    font-size: 24rpx;
    padding: 0 30rpx;
    border-radius: 8rpx;
}

.sku-head {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.sku-endtime {
    display: flex;
}

.sku-price {
    font-size: 24rpx;
    align-items: center;
    margin-top:18rpx;
}

.sku-bar {
    width: 264rpx;
    height: 12rpx;
    background: #eee;
    border-radius: 90px;
}

@keyframes sliderLeft {
    0% {
        transform: scaleX(0);
        transform-origin: 0 0;
    }

    100% {
        transform: scaleX(1);
        transform-origin: 0 0;
    }
}
.sku-title{
    font-size:30rpx;
    color:#333;
}
.sku-rem {
    color: #af1654;
}

.sku-info {
    font-size: 22rpx;
    color: #999;
    width: 264rpx;
    padding:10rpx 0;
}

.sku-bar-active {
    background: #af1654;
    border-radius: 90px;
    height: 12rpx;
    animation: sliderLeft 1s ease-in;
}
.sku-bottom{
    flex: 1;
}

.sku-als{
    width:100%;   
}

.sku-item.disable{
    
}